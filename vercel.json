{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/sw.js",
      "headers": {
        "Content-Type": "application/javascript",
        "Cache-Control": "no-cache"
      }
    },
    {
      "src": "/manifest.json",
      "headers": {
        "Content-Type": "application/manifest+json", 
        "Cache-Control": "no-cache"
      }
    },
    {
      "src": "/(.*)",
      "headers": {
        "Content-Type": "text/html; charset=utf-8"
      }
    }
  ]
}